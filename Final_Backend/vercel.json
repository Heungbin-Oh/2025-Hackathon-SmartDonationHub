{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node",
      "config": {
        "maxDuration": 10
      }
    }
  ],
  "routes": [
    {
      "src": "/api/health",
      "dest": "/server.js",
      "methods": ["GET"],
      "headers": {
        "Cache-Control": "s-maxage=60, stale-while-revalidate"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}
