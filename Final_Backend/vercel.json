{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node",
      "config": {
        "maxDuration": 15
      }
    }
  ],
  "routes": [
    {
      "src": "/api/health",
      "dest": "/server.js",
      "methods": ["GET"],
      "headers": {
        "Cache-Control": "s-maxage=30, stale-while-revalidate"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ],
  "functions": {
    "server.js": {
      "memory": 1024,
      "maxDuration": 15
    }
  }
}
